{
    "ignore": [
        ".git",
        "node_modules/**/node_modules"
    ],
    "watch": [
        "./out/server"
    ],
    "verbose": true,
    "execMap": {
        "rb": "ruby",
        "pde": "processing --sketch={{pwd}} --run",
        "ts": "npx ts-node {{filename}}",
        "js": "node {{filename}}"
    }
    
}